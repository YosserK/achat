pipeline {
    agent any // This runs the pipeline on any available agent

    stages {
        stage('Checkout') {
            steps {
                // Checkout the code from your GitHub repository
                checkout scm
            }
        }
        stage('Build and Test') {
            steps {
                // Run Maven commands to clean and compile code
                sh 'mvn clean compile'
            }
        }
    }
}
