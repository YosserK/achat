    stages {
        stage('Git') {
            steps {
                // Checkout the code from your GitHub repository
                git branch: 'yosser-kaddour', url: 'https://github.com/YosserK/achat.git'
            }
        }

        stage('Maven Clean and Compile') {
            steps {
                // Run Maven clean and compile commands
                sh 'mvn clean compile'
            }
        }
    }

